    "## Here we import the numpy library\n",
    "import numpy as np\n",
    "   \n",
    "## Here we declare the vectors\n",
    "u = np.array([23, 2, 2])\n",
    "v = np.array([40, 10, 1])\n",
    "\n",
    "## Here we find the Euclidean distance\n",
    "np.sqrt(np.sum(np.power(u - v, 2)))"
   ]
  }
